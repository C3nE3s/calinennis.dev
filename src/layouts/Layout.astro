---
import Toolbar from '../components/Toolbar.astro';
export interface Props {
  title?: string;
  desription?: string;
  ogImageSource?: string;
}

const fallbackDescription =
  'I develop software for the modern web. I specialize in Javascript and front-end technologies. This site acts as my personal blog and portfolio.';

const { title, description, ogImageSource } = Astro.props;
---

<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1' />
    <link rel='icon' type='image/x-icon' href='/favicon.ico' />
    <script
      defer
      data-domain='calinennis.dev'
      src='https://plausible.io/js/plausible.js'></script>
    <title>{title ? title + ' - calinennis.dev' : 'calinennis.dev'}</title>
    <meta name='title' content={title ? title : 'calinennis.dev'} />
    <meta name='description' content={description ?? fallbackDescription} />

    <!-- RSS -->
    <!-- <link rel="alternate" type="application/rss+xml" href="/feed/posts.xml"> -->

    <!-- OpenGraph -->
    <meta property='og:title' content={title} />
    <meta property='og:description' content={description ?? fallbackDescription}
    />
    {ogImageSource && <meta property="og:image" content={ogImageSource} />}

    <!-- Twitter -->
    <meta
      name='twitter:card'
      content={ogImageSource ? 'summary_large_image' : 'summary'}
    />
    <meta name='twitter:site' content='@calin_ennis' />
    <meta name='twitter:title' content={title} />
    <meta
      name='twitter:description'
      content={description ?? fallbackDescription}
    />
    {ogImageSource && <meta name="twitter:image" content={ogImageSource} />}
  </head>
  <body>
    <Toolbar />
    <main>
      <slot></slot>
    </main>
    <footer
      class='hidden md:block fixed bottom-2 left-2 p-1 rounded-sm bg-zinc-100/40 backdrop-blur-sm  text-zinc-400 font-mono text-xs tracking-tighter font-light'
    >
      Made with ðŸš€ Astro
    </footer>
    <style>
      html {
        height: 100vh;
        width: 100vw;
        overflow: hidden;
      }

      body {
        position: relative;
        background-color: #fafafa;
        background-image: linear-gradient(
          310deg,
          #fafafa 0%,
          #e4e4e7 30%,
          #d4d4d8 44%,
          #e4e4e7 58%,
          #fafafa 72%
        );
        background-repeat: no-repeat;
        background-size: 525% 525%;
        animation: BgColorRotate 18s cubic-bezier(0.83, 0, 0.17, 1) infinite;
        animation-fill-mode: forwards;
        overflow: hidden;
      }

      body:before {
        content: '';
        position: absolute;
        width: 900%;
        height: 900%;
        top: -450%;
        left: -450%;
        z-index: -1;
        background: url('data:image/svg+xml;base64,')
          0 0 repeat;
        transform: rotate(-33deg);
        opacity: 0.11;
      }

      main {
        max-width: 40rem;
        padding: 2rem;
        margin: auto;
        height: 100vh;
        overflow-y: scroll;
      }

      @keyframes BgColorRotate {
        0% {
          background-position: 10% 0;
        }
        50% {
          background-position: 91% 100%;
        }
        100% {
          background-position: 10% 0;
        }
      }
    </style>
  </body>
</html>
